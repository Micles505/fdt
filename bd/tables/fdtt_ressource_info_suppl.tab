comment on column FDTT_RESSOURCE_INFO_SUPPL.dt_fin
  is 'Date de fin';
comment on column FDTT_RESSOURCE_INFO_SUPPL.nb_hmin_jour
  is 'Nombre d''heure usager  ???? 420';
comment on column FDTT_RESSOURCE_INFO_SUPPL.ind_horaire_variable
